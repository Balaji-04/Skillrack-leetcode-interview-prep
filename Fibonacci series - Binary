INPUT :
6
OUTPUT : 
0 1 1 10 11 101
INPUT :
3
OUTPUT :
0 1 1

#include<stdio.h>
#include<stdlib.h>
 int arr[1002], ai=0;
 int bin (int x){
     ai=0;
     while (x>0){
        int rem = x%2;
        arr[ai++]=rem;
        x/=2;
     }
     return ai;
 }
 int main()
 {
    int n,ini=0,ne=1,c;
    scanf("%d",&n);
     if (n>1)
     printf("0 1 ");
     if (n==1){
        printf("0 ");
     }
    int ai=0;
     for (int i=0;i<n-2;i++){
         c=ini+ne;
         int x = bin(c);
         for (int i = x-1;i>=0;i--)
            printf("%d",arr[i]);
        ini = ne;
        ne=c;
        printf(" ");
     }
 }
